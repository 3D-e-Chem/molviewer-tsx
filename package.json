{
  "name": "jspm-tsx",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "jspm": {
    "name": "molviewer",
    "main": "index.ts",
    "dependencies": {
      "3Dmol": "github:3dmol/3Dmol.js@^1.0.6",
      "@angular/http": "npm:@angular/http@^2.1.0",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.1.0",
      "bootstrap": "github:twbs/bootstrap@^3.3.7",
      "reflect-metadata": "npm:reflect-metadata@^0.1.8"
    },
    "devDependencies": {
      "@types/jquery": "npm:@types/jquery@^2.0.33",
      "@types/mocha": "npm:@types/mocha@^2.2.32",
      "less": "github:systemjs/plugin-less@^0.1.2",
      "mocha": "npm:mocha@^3.1.2",
      "@types/chai": "npm:@types/chai@^3.4.34",
      "chai": "npm:chai@^3.5.0",
      "css": "github:systemjs/plugin-css@^0.1.31",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^5.2.7",
      "text": "github:systemjs/plugin-text@^0.0.9"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@2.1.0",
      "@angular/compiler": "npm:@angular/compiler@2.1.0",
      "@angular/core": "npm:@angular/core@2.1.0",
      "@angular/platform-browser": "npm:@angular/platform-browser@2.1.0",
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "rxjs": "npm:rxjs@5.0.0-beta.12",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "timers": "github:jspm/nodelibs-timers@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zone.js": "npm:zone.js@^0.6.25"
    },
    "overrides": {
      "github:3dmol/3Dmol.js@1.0.6": {
        "format": "global",
        "main": "release/3Dmol-min.js"
      },
      "github:twbs/bootstrap@3.3.7": {
        "main": "js/bootstrap.js",
        "dependencies": {
          "jquery": "jspm:jquery@*"
        },
        "files": [
          "dist",
          "fonts",
          "js",
          "css"
        ],
        "meta": {
          "js/bootstrap.js": {
            "deps": [
              "jquery"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:@angular/common@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/common": ".",
          "./testing": "./bundles/common-testing.umd.js"
        }
      },
      "npm:@angular/compiler@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/compiler": ".",
          "./testing": "./bundles/compiler-testing.umd.js"
        }
      },
      "npm:@angular/core@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/core": ".",
          "./testing": "./bundles/core-testing.umd.js"
        }
      },
      "npm:@angular/http@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/http": ".",
          "./testing": "./bundles/http-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser-dynamic@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser-dynamic": ".",
          "./testing": "./bundles/platform-browser-dynamic-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser": ".",
          "./testing": "./bundles/platform-browser-testing.umd.js"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@3.1.1": {
        "format": "amd"
      },
      "npm:mocha@3.1.2": {
        "main": "./mocha.js",
        "jspmNodeConversion": false,
        "meta": {
          "mocha.js": {
            "deps": [
              "./mocha.css!"
            ],
            "exports": "Mocha",
            "format": "global"
          }
        },
        "dependencies": {
          "css": "*",
          "debug": "^2.2.0",
          "json3": "npm:json3@3.3.2",
          "lodash.create": "npm:lodash.create@3.1.1"
        },
        "registry": "jspm",
        "map": {
          "./mocha.js": {
            "node": "./lib/mocha.js"
          },
          "escape-string-regexp": "@empty",
          "growl": "@empty",
          "./lib/reporters.js": "./lib/reporters/index.js",
          "./lib/interfaces.js": "./lib/interfaces/index.js",
          "./lib/to-iso-string.js": "./lib/to-iso-string/index.js",
          "glob": "@empty",
          "mkdirp": "@empty",
          "jade": "@empty",
          "diff": "@empty",
          "supports-color": "@empty",
          "path": "@node/path",
          "fs": "@node/fs",
          "util": "@node/util",
          "events": "@node/events",
          "tty": "@node/tty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:reflect-metadata@0.1.8": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      },
      "npm:typescript@2.0.3": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "jspm install",
    "tslint": "tslint --config tslint.json src/**/*.ts src/**/*.tsx",
    "clean": "npm run dist:clean && npm run test:clean",
    "dist:clean": "rm -rf dist",
    "dist:prep": "mkdir dist || true",
    "dist:bundle-deps": "jspm bundle src/molviewer.tsx - [src/**/*] dist/dependency-bundle.js --minify --skip-source-maps",
    "dist:bundle-app": "jspm bundle src/molviewer.tsx - dist/dependency-bundle.js dist/app-bundle.js --skip-source-maps # TODO add --minify, but with flag got Error: SyntaxError: Unexpected token: name (HelloWorld)",
    "dist:copy:systemjs": "cp jspm_packages/system.js jspm.config.js dist/",
    "dist:copy:html": "cp index-dist.html dist/index.html",
    "dist:copy": "npm run dist:copy:html && npm run dist:copy:systemjs",
    "dist": "npm run dist:prep && npm run dist:bundle-deps && npm run dist:bundle-app && npm run dist:copy",
    "test:clean": "rm -rf test_results",
    "test": "karma start"
  },
  "author": "me",
  "license": "Apache-2.0",
  "devDependencies": {
    "3Dmol": "github:3dmol/3Dmol.js",
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/http": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.34",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/mocha": "^2.2.32",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "isomorphic-fetch": "^2.2.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jspm": "github:malaupa/karma-jspm",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "mocha": "^3.1.0",
    "rxjs": "^5.0.0-beta.12",
    "tslint": "^4.0.0-dev.0",
    "typescript": "^2.0.3",
    "zone.js": "^0.6.25"
  }
}
