{
  "name": "jspm-tsx",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "jspm": {
    "name": "molviewer",
    "main": "molviewer.js",
    "dependencies": {
      "3Dmol": "github:3dmol/3Dmol.js@^1.0.6",
      "@types/isomorphic-fetch": "npm:@types/isomorphic-fetch@^0.0.31",
      "bootstrap": "github:twbs/bootstrap@^3.3.7",
      "isomorphic-fetch": "npm:isomorphic-fetch@^2.2.1",
      "jquery": "npm:jquery@^3.1.1",
      "react-bootstrap": "npm:react-bootstrap@^0.30.5",
      "react-redux": "npm:react-redux@^4.4.5",
      "redux-saga": "npm:redux-saga@^0.12.0"
    },
    "devDependencies": {
      "@types/chai": "npm:@types/chai@^3.4.34",
      "@types/jquery": "npm:@types/jquery@^2.0.33",
      "@types/mocha": "npm:@types/mocha@^2.2.32",
      "@types/react": "npm:@types/react@^0.14.39",
      "@types/react-bootstrap": "npm:@types/react-bootstrap@^0.0.33",
      "@types/react-dom": "npm:@types/react-dom@^0.14.17",
      "@types/react-redux": "npm:@types/react-redux@^4.4.32",
      "@types/redux": "npm:@types/redux@^3.6.31",
      "@types/redux-saga": "npm:@types/redux-saga@^0.9.30",
      "chai": "npm:chai@^3.5.0",
      "css": "github:systemjs/plugin-css@^0.1.31",
      "less": "github:systemjs/plugin-less@^0.1.2",
      "mocha": "npm:mocha@^3.1.0",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^5.2.7"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "domain": "github:jspm/nodelibs-domain@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "react": "npm:react@^15.3.2",
      "react-dom": "npm:react-dom@^15.3.2",
      "redux": "npm:redux@^3.6.0",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
    },
    "overrides": {
      "github:3dmol/3Dmol.js@1.0.6": {
        "format": "global",
        "main": "release/3Dmol-min.js"
      },
      "github:twbs/bootstrap@3.3.7": {
        "main": "js/bootstrap.js",
        "dependencies": {
          "jquery": "jspm:jquery@*"
        },
        "files": [
          "dist",
          "fonts",
          "js",
          "css"
        ],
        "meta": {
          "js/bootstrap.js": {
            "deps": [
              "jquery"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@3.1.1": {
        "format": "amd"
      },
      "npm:lodash@4.16.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:mocha@3.1.0": {
        "main": "./mocha.js",
        "jspmNodeConversion": false,
        "meta": {
          "mocha.js": {
            "deps": [
              "./mocha.css!"
            ],
            "exports": "Mocha",
            "format": "global"
          }
        },
        "dependencies": {
          "css": "*",
          "debug": "^2.2.0",
          "json3": "npm:json3@3.3.2",
          "lodash.create": "npm:lodash.create@3.1.1"
        },
        "registry": "jspm",
        "map": {
          "./mocha.js": {
            "node": "./lib/mocha.js"
          },
          "escape-string-regexp": "@empty",
          "growl": "@empty",
          "./lib/reporters.js": "./lib/reporters/index.js",
          "./lib/interfaces.js": "./lib/interfaces/index.js",
          "./lib/to-iso-string.js": "./lib/to-iso-string/index.js",
          "glob": "@empty",
          "mkdirp": "@empty",
          "jade": "@empty",
          "diff": "@empty",
          "supports-color": "@empty",
          "path": "@node/path",
          "fs": "@node/fs",
          "util": "@node/util",
          "events": "@node/events",
          "tty": "@node/tty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:typescript@2.0.3": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "jspm install",
    "tslint": "tslint --config tslint.json src/**/*.ts src/**/*.tsx",
    "clean": "npm run dist:clean && npm run test:clean",
    "dist:clean": "rm -rf dist",
    "dist:prep": "mkdir dist || true",
    "dist:bundle-deps": "jspm bundle src/molviewer.tsx - [src/**/*] dist/dependency-bundle.js --minify --skip-source-maps",
    "dist:bundle-app": "jspm bundle src/molviewer.tsx - dist/dependency-bundle.js dist/app-bundle.js --skip-source-maps # TODO enable --minify, now disabled because of generators. Option1 polyfill+target:es5",
    "dist:copy:systemjs": "cp jspm_packages/system.js jspm.config.js dist/",
    "dist:copy:html": "cp index-dist.html dist/index.html",
    "dist:copy:fonts": "mkdir -p dist/jspm_packages/github/twbs/bootstrap@3.3.7/dist && cp -r jspm_packages/github/twbs/bootstrap@3.3.7/dist/fonts dist/jspm_packages/github/twbs/bootstrap@3.3.7/dist/",
    "dist:copy": "npm run dist:copy:html && npm run dist:copy:systemjs && npm run dist:copy:fonts",
    "dist": "npm run dist:prep && npm run dist:bundle-deps && npm run dist:bundle-app && npm run dist:copy",
    "test:clean": "rm -rf test_results",
    "test": "karma start"
  },
  "author": "me",
  "license": "Apache-2.0",
  "devDependencies": {
    "3Dmol": "github:3dmol/3Dmol.js",
    "@types/chai": "^3.4.34",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/jquery": "^2.0.33",
    "@types/mocha": "^2.2.32",
    "@types/react": "^0.14.39",
    "@types/react-bootstrap": "0.0.33",
    "@types/react-dom": "^0.14.17",
    "@types/react-redux": "^4.4.32",
    "@types/redux": "^3.6.31",
    "@types/redux-saga": "^0.9.30",
    "chai": "^3.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jspm": "github:malaupa/karma-jspm",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "mocha": "^3.1.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-saga": "^0.12.0",
    "tslint": "^4.0.0-dev.0",
    "tslint-react": "^1.0.0",
    "typescript": "^2.1.0-dev.20161009"
  }
}
